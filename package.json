    "http-status": "^1.7.4",
    "joi": "^17.13.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.14.10",
    "@types/supertest": "^6.0.3",
    "dotenv": "^17.2.2",
    "dotenv-cli": "^10.0.0",
    "jest": "^30.2.0",
    "jest-esm-transformer": "^1.0.0",
